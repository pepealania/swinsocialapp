                                                <FormattedString>
                                                    <Span Text="{Binding MyProfile.UserName}" TextColor="White" FontSize="20" FontAttributes="Bold" BackgroundColor="Black"></Span>
                                                    <Span Text=", "></Span>
                                                    <Span Text="{Binding MyProfile.Age}" TextColor="White" FontSize="20" BackgroundColor="black"></Span>
                                                </FormattedString>
                                            </Label.FormattedText>
                                        </Label>
                                        <StackLayout>
                                            <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="EditUserNameAge_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                        </StackLayout>
                                    </StackLayout>
                                    <StackLayout Orientation="Horizontal">
                                        <StackLayout>
                                            <Label FontSize="16" TextColor="White" Margin="5" BackgroundColor="#000000">Tagline</Label>

                                            <Frame CornerRadius="5" BackgroundColor="#121213" IsVisible="true">
                                                <Label Text="{Binding MyProfile.Tagline}" TextColor="white" FontSize="12"></Label>
                                            </Frame>
                                        </StackLayout>
                                        <StackLayout>
                                            <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="Tegline_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                        </StackLayout>

                                    </StackLayout>
                                    <StackLayout Orientation="Horizontal">
                                        <Label BackgroundColor="Black" FontSize="16" FontAttributes="Bold" Margin="20,0,0,20" TextColor="White" VerticalOptions="End">
                                            <Label.FormattedText>
                                                <FormattedString>
                                                    <Span Text="{Binding MyProfile.Location}" TextColor="#D30CDB" BackgroundColor="Black" FontSize="16"></Span>
                                                </FormattedString>
                                            </Label.FormattedText>
                                        </Label>
                                        <StackLayout>
                                            <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="Location_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                        </StackLayout>
                                    </StackLayout>

                                    <Frame Padding="2" IsVisible="{Binding MyProfile.SwingStyleTagsObjectListIsVisible}" CornerRadius="10" BackgroundColor="#000000">
