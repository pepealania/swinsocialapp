                                            <StackLayout >
                                                <Label FontSize="16" TextColor="White" Margin="5" BackgroundColor="#000000">About</Label>
                                                <htmllabel:HtmlLabel Text="{Binding MyProfile.About}" TextColor="White"></htmllabel:HtmlLabel>
                                            </StackLayout>
                                            <StackLayout>
                                                <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="About_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                            </StackLayout>
                                        </StackLayout>

                                        <!--</Frame>-->
                                    </Frame>

                                    <Frame Padding="2" CornerRadius="10" BackgroundColor="black" IsVisible="true">
