            AbsoluteLayout.LayoutFlags="PositionProportional" Aspect="Fill"  HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand"/>
                                            <Frame Padding="5"                                             Margin="1"
       BorderColor="Black"
       CornerRadius="50"
       HeightRequest="30"
       WidthRequest="30"
       IsClippedToBounds="True"
       HorizontalOptions="Center"
       VerticalOptions="Center" AbsoluteLayout.LayoutBounds="280,0, 80, 80" BackgroundColor="#292a2e">
                                                <ImageButton BackgroundColor="#292a2e" Source="writeemail" Aspect="AspectFit"                                           Margin="0"
HeightRequest="30"
WidthRequest="30" Clicked="EditProfileBanner_Clicked"></ImageButton>
                                            </Frame>
                                            <Frame
                                            Margin="10"
       BorderColor="Black"
       CornerRadius="50"
       HeightRequest="80"
       WidthRequest="80"
       IsClippedToBounds="True"
