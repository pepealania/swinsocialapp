                                        <StackLayout Orientation="Horizontal">
                                            <StackLayout BindableLayout.ItemsSource="{Binding MyProfile.SwingStyleTagsObjectList}" Orientation="Horizontal" Spacing="1" HorizontalOptions="Start" Padding="0" >
                                                <!--<Label
                                                           TextColor="black"
                                                           Text="one"
                                                                BackgroundColor="Yellow" WidthRequest="20" Margin="10"
                                                           />
                                                <Label
                                                           TextColor="black"
                                                           Text="two"
                                                                BackgroundColor="Yellow" WidthRequest="20" Margin="10"
                                                           />-->
                                                <!--<CollectionView HeightRequest="400" ItemsSource="{Binding SwingStyleTagsObjectList}">
                                                    <CollectionView.ItemTemplate >-->
                                                <BindableLayout.ItemTemplate>
                                                    <DataTemplate x:DataType="model:SwingStyleTagsObject">
                                                        <StackLayout>
                                                            <Grid HorizontalOptions="Center" VerticalOptions="Center">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="25"></RowDefinition>
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Frame CornerRadius="10" Padding="0" BackgroundColor="#343A40" Margin="2">
                                                                    <Label HorizontalOptions="Center" VerticalOptions="Center"
                                                        TextColor="white"
                                                        Text="{Binding Description}"
                                                             BackgroundColor="#343A40"
                                                        />
                                                                </Frame>
                                                            </Grid>
                                                        </StackLayout>
                                                    </DataTemplate>
                                                </BindableLayout.ItemTemplate>

                                                <!--</CollectionView.ItemTemplate>
                                                </CollectionView>-->

                                            </StackLayout>
                                            <StackLayout>
                                                <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="SwingStyles_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                            </StackLayout>
                                        </StackLayout>
                                        <!--</Frame>-->
                                    </Frame>
                                    <StackLayout>
                                        <!--<Image Source="ProfileMockButtons.png" HorizontalOptions="CenterAndExpand"></Image>-->
                                    </StackLayout>
                                    <Frame Padding="2" CornerRadius="10" BackgroundColor="black" IsVisible="true">
