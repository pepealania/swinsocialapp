                                            <StackLayout >
                                                <Label FontSize="16" TextColor="White" Margin="5" BackgroundColor="#000000">Account type</Label>
                                                <htmllabel:HtmlLabel Text="{Binding MyProfile.AccountType}" TextColor="White"></htmllabel:HtmlLabel>
                                            </StackLayout>
                                            <StackLayout>
                                                <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="EditAccountType_Clicked"  CommandParameter="{Binding MyProfile}"></ImageButton>
                                            </StackLayout>
                                        </StackLayout>

                                        <!--</Frame>-->
                                    </Frame>

                                    <StackLayout Orientation="Horizontal" Margin="10,20,0,0" HorizontalOptions="FillAndExpand" BackgroundColor="black">
                                        <Label Text="Details" FontSize="16" TextColor="#ffffff" ></Label>
                                        <StackLayout HorizontalOptions="EndAndExpand">
                                            <ImageButton BackgroundColor="#292a2e" HeightRequest="30" WidthRequest="30" Source="writeemail" Padding="5" Clicked="Details_Clicked" CommandParameter="{Binding MyProfile}"></ImageButton>
                                        </StackLayout>
                                    </StackLayout>
                                    <WebView Source="{Binding MyProfile.DetailsHtmlTable}" HeightRequest="300" BackgroundColor="Black">

                                    </WebView>

                                    <StackLayout Orientation="Horizontal" Margin="10,20,0,0" HorizontalOptions="FillAndExpand" BackgroundColor="black" IsVisible="{Binding MyProfile.ShowPartnerDetailsTitle}">
