         WidthRequest="80" />
                                            </Frame>
                                            <Frame Padding="5"                                             Margin="1"
       BorderColor="Black"
       CornerRadius="50"
       HeightRequest="30"
       WidthRequest="30"
       IsClippedToBounds="True"
       HorizontalOptions="Center"
       VerticalOptions="Center" AbsoluteLayout.LayoutBounds="70,50, 80, 80" BackgroundColor="#292a2e">
