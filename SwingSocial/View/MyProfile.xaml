<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SwingSocial.Sample.View.MyProfilePage"
                          xmlns:model="clr-namespace:SwingSocial.Sample.Model"
xmlns:viewmodel="clr-namespace:SwingSocial.Sample.ViewModel" x:DataType="viewmodel:MyProfileViewModel"
             xmlns:maps="clr-namespace:Xamarin.Forms.GoogleMaps;assembly=Xamarin.Forms.GoogleMaps"    
             xmlns:skia="clr-namespace:SkiaSharp.Views.Forms;assembly=SkiaSharp.Views.Forms"  xmlns:htmllabel="clr-namespace:Plugin.HtmlLabel;assembly=Plugin.HtmlLabel"
             BackgroundColor="Black"
             Title ="" Shell.BackgroundColor="Black">
    <Shell.TitleView>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>

            <Image Source="thumbnail_logo.png" WidthRequest="205" HeightRequest="45" HorizontalOptions="Center" BackgroundColor="Transparent" Grid.ColumnSpan="3" Margin="0,0,0,0"/>
            

        </Grid>

    </Shell.TitleView>
    <ContentPage.Content>
        <StackLayout BackgroundColor="Black" x:Name="TopStackLayout">
            <StackLayout Margin="20" BackgroundColor="Black">
                <AbsoluteLayout VerticalOptions="FillAndExpand">
                    <ScrollView Orientation="Vertical" x:Name="MainScroll"
                            AbsoluteLayout.LayoutBounds="0,0,1,1" AbsoluteLayout.LayoutFlags="All" >
                        <StackLayout x:Name="ProfileSection" BackgroundColor="black" Grid.Row="0" WidthRequest="150" HeightRequest="800" IsVisible="true">
                            <Button CornerRadius="10" Text="Logout" TextColor="White" Background="#292a2e" Clicked="Logout_Tapped" HorizontalOptions="Start">
                            </Button>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="50"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3.3*"></ColumnDefinition>
                                    <ColumnDefinition Width="3.3*"></ColumnDefinition>
                                    <ColumnDefinition Width="3.3*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Frame Grid.Column="0" BackgroundColor="#c21164" CornerRadius="10" Padding="5" Margin="5">
                                    <Label BackgroundColor="#c21164" TextColor="White" Text="Billing" HorizontalOptions="Center">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Billing_Tapped"></TapGestureRecognizer>
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Frame>
                                <Frame Grid.Column="1" BackgroundColor="#c21164" CornerRadius="10" Padding="5" Margin="5">
                                    <Label BackgroundColor="#c21164" TextColor="White" Text="Preferences" HorizontalOptions="Center">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Preferences_Tapped"></TapGestureRecognizer>
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Frame>
                                <Frame Grid.Column="2" BackgroundColor="#c21164" CornerRadius="10" Padding="5" Margin="5">
                                    <Label BackgroundColor="#c21164" TextColor="White" TextType="Html" x:Name="AvailableButtonLabel" HorizontalOptions="Center" Text="{Binding MyProfile.AvailabilityButtonLabel}">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Available_Tapped"></TapGestureRecognizer>
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Frame>
                            </Grid>
