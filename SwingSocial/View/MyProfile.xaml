                                        </StackLayout>
                                    </StackLayout>
                                    <WebView Source="{Binding MyProfile.PartnerDetailsHtmlTable}" HeightRequest="300" BackgroundColor="Black" IsVisible="{Binding MyProfile.ShowPartnerDetailsHtmlTable}">

                                    </WebView>
                                    <StackLayout Orientation="Horizontal" Margin="10,20,0,0" HorizontalOptions="FillAndExpand" BackgroundColor="black">
                                        <Label Text="Update Checked Public Images (Available to Everyone)" FontSize="16" TextColor="#ffffff" ></Label>
                                    </StackLayout>

